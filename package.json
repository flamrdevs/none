{
  "name": "none",
  "version": "0.0.0",
  "description": "Colorful SVG generation",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsdown --env.NODE_ENV development",
    "build": "tsdown --env.NODE_ENV production",
