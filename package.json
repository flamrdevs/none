  "devDependencies": {
    "@types/node": "22.15.18",
    "hono": "4.7.9",
    "satori": "0.13.1",
    "string-ts": "2.2.1",
    "tsdown": "0.11.9",
    "typescript": "5.8.3",
    "valibot": "1.1.0"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=23.0.0"
  }
}
